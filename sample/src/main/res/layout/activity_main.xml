<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".AnayticsEventsTracker">

    <Button
        android:layout_margin="16dp"
        android:id="@+id/sendEvent1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Send Event One" />

    <Button
        android:id="@+id/sendEvent2"
        android:layout_margin="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sendEvent1"
        android:text="Send Event Two" />

    <Button
        android:id="@+id/sendEvent3"
        android:layout_margin="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sendEvent2"
        android:text="Send Event Three" />

    <Button
        android:id="@+id/startActivityBtn"
        android:layout_margin="16dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/sendEvent3"
        android:text="Send Event Three" />

</RelativeLayout>